        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dictionar"
        xmlns:Components="clr-namespace:Dictionar.Components" x:Class="Dictionar.MainWindow"
        xmlns:lists="clr-namespace:Dictionar.MyClasses"
        mc:Ignorable="d"
        Title="MeowDictionary" Width="980" Height="570" ResizeMode="CanMinimize"
        BorderBrush="Black" BorderThickness="2">
    <Window.DataContext>
        <lists:Words/>
    </Window.DataContext>
    <Grid>
        <Frame x:Name="mainFrame" Source="/Components/SearchPage.xaml" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
